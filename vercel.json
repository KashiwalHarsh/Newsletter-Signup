{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      },
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "style.css"
      },
      {
        "src": "/(.*)",
        "dest": "/logo.png"
      }

    ]
  }